def turn_right():
    turn_left()
    turn_left()
    turn_left()

while not at_goal():
    if wall_in_front():
        turn_left()
        move()
        turn_right()
    if front_is_clear():
        move()
        turn_right()
        while not wall_in_front():
            move()
        if wall_in_front():
            turn_left()
